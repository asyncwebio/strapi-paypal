{
  "name": "strapi-paypal",
  "version": "1.0.3",
  "description": "Online payments and subscriptions made simple, secure and fast.This free plugin enables you to accept online payments and create subscriptions using Paypal account or Credit/Debit card on your Strapi app via Paypal.",
  "strapi": {
    "name": "strapi-paypal",
    "description": "Online payments and subscriptions made simple, secure and fast.This free plugin enables you to accept online payments and create subscriptions using Paypal account or Credit/Debit card on your Strapi app via Paypal.",
    "kind": "plugin",
    "displayName": "Strapi Paypal"
  },
  "dependencies": {
    "html-react-parser": "^3.0.4",
    "moment": "^2.29.4",
    "qs": "^6.11.0"
  },
  "peerDependencies": {
    "@strapi/strapi": "^4.4.5"
  },
  "scripts": {
    "format": "npm-run-all -p format:*",
    "format:code": "npm run prettier:code -- --write",
    "format:other": "npm run prettier:other -- --write",
    "lint": "npm-run-all -p lint:code",
    "lint:code": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:other": "npm run prettier:other -- --check",
    "prettier:code": "prettier \"**/*.js\"",
    "prettier:other": "prettier \"**/*.{md,css,scss,yaml,yml}\""
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-react": "^7.16.7",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.9.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "3.0.0",
    "lint-staged": "10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md,css,scss,yaml,yml}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "author": {
    "name": "AsyncWeb Technology",
    "email": "support@asyncweb.io",
    "url": "https://higheredlab.com/"
  },
  "homepage": "https://github.com/AsyncWeb/strapi-paypal#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/AsyncWeb/strapi-paypal.git"
  },
  "maintainers": [
    {
      "name": "AsyncWeb Technology",
      "email": "support@asyncweb.io",
      "url": "https://higheredlab.com/"
    }
  ],
  "engines": {
    "node": ">=12.x.x <=16.x.x",
    "npm": ">=6.0.0"
  },
